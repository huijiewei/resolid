{
  "name": "@resolid/remix-plugins",
  "version": "3.0.3",
  "type": "module",
  "sideEffects": false,
  "description": "Resolid Remix 扩展插件",
  "author": "Huijie Wei",
  "license": "MIT",
  "keywords": ["resolid", "remix", "vite"],
